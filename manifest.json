{
    "name": "WVSpotify_beta",
    "description": "Play and control a music from Spotify in different places without vpn",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    },
    "action": {
        "default_title": "Click me to open an app",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "64": "icons/64.png",
            "128": "icons/128.png",
            "256": "icons/256.png",
            "512": "icons/512.png"
        }
    },
    "permissions": [
        "storage",
        "background",
        "management",
        "activeTab",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "app/background.js"
    }
}